/*#region Phone Subnavigation */

#phoneSubNavigation {
    padding-left: 20px;
    padding-right: 20px;
    padding-top: 10px;
    padding-bottom: 10px;
    background-color: black;
}

    #phoneSubNavigation ul {
        list-style-type: none;
        padding: 0;
        margin-left: 0;
        text-align: center;
    }


    #phoneSubNavigation li {
        margin-top: 10px;
        margin-bottom: 5px;
        line-height: 40px;
        background: #2b579e; /* Old browsers */
        background: -moz-linear-gradient(top, #2b579e 1%, #093c8b 50%, #18245d 100%); /* FF3.6+ */
        background: -webkit-gradient(linear, left top, left bottom, color-stop(1%,#2b579e), color-stop(50%,#093c8b), color-stop(100%,#18245d)); /* Chrome,Safari4+ */
        background: -webkit-linear-gradient(top, #2b579e 1%,#093c8b 50%,#18245d 100%); /* Chrome10+,Safari5.1+ */
        background: -o-linear-gradient(top, #2b579e 1%,#093c8b 50%,#18245d 100%); /* Opera 11.10+ */
