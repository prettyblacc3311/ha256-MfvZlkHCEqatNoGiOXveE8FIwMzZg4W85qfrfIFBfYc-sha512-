/*header menu overrides*/

#headerMenu {
    background-image: url('images/top-right-nav.png');
    background-position: center;
    background-size: 100% 100%; /* This is to get around an artifact in Chrome when zooming the page. */
    height: 20px;
    width: 160px;
    color: #000000;
    line-height: 0px;
    padding-right: 0px;
    padding-top: 0px;
    padding-bottom: 0px;
