/*#region Collapsible phone menu */

#phoneHeader {
    background-image: url('Images/header_logo_menu_bg_480x55.jpg');
    background-repeat: no-repeat;
    background-color: #224189;
}

.shadow {
    -moz-box-shadow: 3px 3px 4px #000;
    -webkit-box-shadow: 3px 3px 4px #000;
    box-shadow: 3px 3px 4px #000;
    /* For IE 8 */
    -ms-filter: "progid:DXImageTransform.Microsoft.Shadow(Strength=4, Direction=135, Color='#000000')";
    /* For IE 5.5 - 7 */
    filter: progid:DXImageTransform.Microsoft.Shadow(Strength=4, Direction=135, Color='#000000');
}

#phoneMenu {
    position: fixed;
    /*-webkit-backface-visibility: hidden;*/
    background-color: #276CB2;
    top: 0;
    bottom: 0;
    left: 0;
    -webkit-transition: left 0.35s ease;
    -moz-transition: left 0.35s ease;
    -o-transition: left 0.35s ease;
    transition: left 0.35s ease;
    width: 250px;
}

    #phoneMenu.closed {
        left: -250px;
    }




#phoneMenuLink {
    height: 100%;
    width: 15%;
    display: inline-block;
}


/*#region phone side menu styling */

#phoneMenuWrapper {
    background-color: #276CB2;
    line-height: 24px;
}


.btn-right-arrow {
    background-image: url('Images/arrow_right.png');
    background-color: transparent;
    background-repeat: no-repeat;
    background-position: center, center;
    border: 0;
    height: 100%;
    width: 20px;
}

.btn-down-arrow {
    background-image: url('Images/arrow_down.png');
    background-color: transparent;
    background-repeat: no-repeat;
    background-position: center, center;
    border: 0;
    height: 100%;
    width: 20px;
}

.phone-menu-level-one {
    height: 26px;
    border-top: 1px solid #699ACA;
    background-color: #276CB2;
    display: inline-block;
    width: 100%;
    margin: 0;
    line-height: 26px;
}

    .phone-menu-level-one a {
